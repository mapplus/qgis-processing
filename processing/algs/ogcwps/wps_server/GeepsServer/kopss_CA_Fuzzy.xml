<?xml version="1.0" encoding="utf-8"?>
<wps:ProcessDescriptions xml:lang="en" service="WPS" version="1.0.0" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsAll.xsd" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink">
  <ProcessDescription wps:processVersion="1.0.0" statusSupported="true" storeSupported="true">
    <ows:Identifier>kopss:CA_Fuzzy</ows:Identifier>
    <ows:Title>CA_Fuzzy</ows:Title>
    <ows:Abstract>Transforms the input raster into a 0 to 1 scale, indicating the strength of a membership in a set, based on a specified fuzzification algorithm.</ows:Abstract>
    <DataInputs>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>inputCoverage</ows:Identifier>
        <ows:Title>The input gridcoverage to be processed.</ows:Title>
        <ows:Abstract>The input gridcoverage to be processed</ows:Abstract>
        <ComplexData>
          <Default>
            <Format>
              <MimeType>application/arcgrid</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>application/arcgrid</MimeType>
            </Format>
            <Format>
              <MimeType>image/geotiff</MimeType>
            </Format>
            <Format>
              <MimeType>image/tiff</MimeType>
            </Format>
          </Supported>
        </ComplexData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>fuzzyType</ows:Identifier>
        <ows:Title>Fuzzy membership function</ows:Title>
        <ows:Abstract>LinearCutoff(default), LinearOneTrangular, LinearTwoTrangular, CurveSigmoidal, CurveGaussian, CurveBell</ows:Abstract>
        <LiteralData>
          <ows:AllowedValues>
            <ows:Value>LinearCutoff</ows:Value>
            <ows:Value>LinearOneTrangular</ows:Value>
            <ows:Value>LinearTwoTrangular</ows:Value>
            <ows:Value>CurveSigmoidal</ows:Value>
            <ows:Value>CurveGaussian</ows:Value>
            <ows:Value>CurveBell</ows:Value>
          </ows:AllowedValues>
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>isDecreasing</ows:Identifier>
        <ows:Title>isDecreasing</ows:Title>
        <ows:Abstract>True or False</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:boolean</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>minValue</ows:Identifier>
        <ows:Title>minValue</ows:Title>
        <ows:Abstract>minValue</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>maxValue</ows:Identifier>
        <ows:Title>maxValue</ows:Title>
        <ows:Abstract>maxValue</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>midValue1</ows:Identifier>
        <ows:Title>midValue1</ows:Title>
        <ows:Abstract>midValue1: 0(Default)</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>midValue2</ows:Identifier>
        <ows:Title>midValue2</ows:Title>
        <ows:Abstract>midValue2: 0(Default)</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>maxRange</ows:Identifier>
        <ows:Title>maxRange</ows:Title>
        <ows:Abstract>maxRange: 0 ~ maxRange, Default = 1</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
    </DataInputs>
    <ProcessOutputs>
      <Output>
        <ows:Identifier>result</ows:Identifier>
        <ows:Title>Result</ows:Title>
        <ComplexOutput>
          <Default>
            <Format>
              <MimeType>application/arcgrid</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>application/arcgrid</MimeType>
            </Format>
            <Format>
              <MimeType>image/geotiff</MimeType>
            </Format>
            <Format>
              <MimeType>image/tiff</MimeType>
            </Format>
          </Supported>
        </ComplexOutput>
      </Output>
    </ProcessOutputs>
  </ProcessDescription>
</wps:ProcessDescriptions>