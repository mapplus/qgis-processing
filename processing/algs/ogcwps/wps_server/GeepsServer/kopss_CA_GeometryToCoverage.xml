<?xml version="1.0" encoding="utf-8"?>
<wps:ProcessDescriptions xml:lang="en" service="WPS" version="1.0.0" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsAll.xsd" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink">
  <ProcessDescription wps:processVersion="1.0.0" statusSupported="true" storeSupported="true">
    <ows:Identifier>kopss:CA_GeometryToCoverage</ows:Identifier>
    <ows:Title>CA_GeometryToCoverage</ows:Title>
    <ows:Abstract>Converts features to a gridcoverage</ows:Abstract>
    <DataInputs>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>inputGeometry</ows:Identifier>
        <ows:Title>Input geometry to be converted</ows:Title>
        <ows:Abstract>inputGeometry: Polygon feature type</ows:Abstract>
        <ComplexData>
          <Default>
            <Format>
              <MimeType>text/xml; subtype=gml/3.1.1</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>text/xml; subtype=gml/3.1.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=gml/2.1.2</MimeType>
            </Format>
            <Format>
              <MimeType>application/wkt</MimeType>
            </Format>
            <Format>
              <MimeType>application/gml-3.1.1</MimeType>
            </Format>
            <Format>
              <MimeType>application/gml-2.1.2</MimeType>
            </Format>
            <Format>
              <MimeType>application/wkb</MimeType>
            </Format>
            <Format>
              <MimeType>application/json</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=kml/2.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=kml/2.2</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=georss</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=atom</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=svg</MimeType>
            </Format>
          </Supported>
        </ComplexData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>forcedCRS</ows:Identifier>
        <ows:Title>Output forced CRS</ows:Title>
        <ows:Abstract>Forces a certain CRS on features before reprojection ex)EPSG:4326</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>pixelType</ows:Identifier>
        <ows:Title>Output pixel type: 1(Integer, default), 2(Float)</ows:Title>
        <ows:Abstract>pixelType: 1(Integer, default), 2(Float)</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>defaultValue</ows:Identifier>
        <ows:Title>Pixel's default value: 1(default)</ows:Title>
        <ows:Abstract>defaultValue: default(1)</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>cellSize</ows:Identifier>
        <ows:Title>The cell size for the output gridcoverage</ows:Title>
        <ows:Abstract>The cell size for the output gridcoverage</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
    </DataInputs>
    <ProcessOutputs>
      <Output>
        <ows:Identifier>result</ows:Identifier>
        <ows:Title>Result</ows:Title>
        <ComplexOutput>
          <Default>
            <Format>
              <MimeType>application/arcgrid</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>application/arcgrid</MimeType>
            </Format>
            <Format>
              <MimeType>image/geotiff</MimeType>
            </Format>
            <Format>
              <MimeType>image/tiff</MimeType>
            </Format>
          </Supported>
        </ComplexOutput>
      </Output>
    </ProcessOutputs>
  </ProcessDescription>
</wps:ProcessDescriptions>