<?xml version="1.0" encoding="utf-8"?>
<wps:ProcessDescriptions xml:lang="en" service="WPS" version="1.0.0" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsAll.xsd" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink">
  <ProcessDescription wps:processVersion="1.0.0" statusSupported="true" storeSupported="true">
    <ows:Identifier>kopss:CA_GetMap</ows:Identifier>
    <ows:Title>CA_GetMap</ows:Title>
    <ows:Abstract>A CA_GetMap request retrieves a map from inputCoverage and sld</ows:Abstract>
    <DataInputs>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>inputCoverage</ows:Identifier>
        <ows:Title>inputCoverage</ows:Title>
        <ows:Abstract>inputCoverage</ows:Abstract>
        <ComplexData>
          <Default>
            <Format>
              <MimeType>application/arcgrid</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>application/arcgrid</MimeType>
            </Format>
            <Format>
              <MimeType>image/geotiff</MimeType>
            </Format>
            <Format>
              <MimeType>image/tiff</MimeType>
            </Format>
          </Supported>
        </ComplexData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>style</ows:Identifier>
        <ows:Title>Styled Layer Descriptors</ows:Title>
        <ows:Abstract>SLD</ows:Abstract>
        <ComplexData>
          <Default>
            <Format>
              <MimeType>text/xml; subtype=sld/1.1.0</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>text/xml; subtype=sld/1.1.0</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=sld/1.0.0</MimeType>
            </Format>
          </Supported>
        </ComplexData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>bBox</ows:Identifier>
        <ows:Title>Bounding box</ows:Title>
        <ows:Abstract>Bounding box corners (lower left, upper right): minx,miny,maxx,maxy</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>crs</ows:Identifier>
        <ows:Title>crs</ows:Title>
        <ows:Abstract>Coordinate Reference System</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>width</ows:Identifier>
        <ows:Title>Width</ows:Title>
        <ows:Abstract>Width in pixels of resulting map image</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:int</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>height</ows:Identifier>
        <ows:Title>Height</ows:Title>
        <ows:Abstract>Height in pixels of resulting map image</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:int</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>format</ows:Identifier>
        <ows:Title>Output format of map</ows:Title>
        <ows:Abstract>Output format of map. Valid values are image/jpeg, image/png, and image/gif</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>transparent</ows:Identifier>
        <ows:Title>Map background transparency</ows:Title>
        <ows:Abstract>Map background transparency. Default is False</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>bgColor</ows:Identifier>
        <ows:Title>Background color</ows:Title>
        <ows:Abstract>Hexidecimal red-blue-green color value for the map background color. Default is 0xFFFFFF (white)</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
    </DataInputs>
    <ProcessOutputs>
      <Output>
        <ows:Identifier>result</ows:Identifier>
        <ows:Title>Result</ows:Title>
        <ComplexOutput>
          <Default>
            <Format>
              <MimeType>image/png</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>image/png</MimeType>
            </Format>
          </Supported>
        </ComplexOutput>
      </Output>
    </ProcessOutputs>
  </ProcessDescription>
</wps:ProcessDescriptions>