<?xml version="1.0" encoding="utf-8"?>
<wps:ProcessDescriptions xml:lang="en" service="WPS" version="1.0.0" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsAll.xsd" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink">
  <ProcessDescription wps:processVersion="1.0.0" statusSupported="true" storeSupported="true">
    <ows:Identifier>kopss:CA_IDW</ows:Identifier>
    <ows:Title>CA_IDW</ows:Title>
    <ows:Abstract>Interpolates a surface from points using an inverse distance weighted (IDW) technique.</ows:Abstract>
    <DataInputs>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>inputFeatures</ows:Identifier>
        <ows:Title>Input features</ows:Title>
        <ows:Abstract>The input point features containing the z-values to be interpolated into a surface raster.</ows:Abstract>
        <ComplexData>
          <Default>
            <Format>
              <MimeType>text/xml; subtype=wfs-collection/1.1</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>text/xml; subtype=wfs-collection/1.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=wfs-collection/1.0</MimeType>
            </Format>
            <Format>
              <MimeType>application/json</MimeType>
            </Format>
            <Format>
              <MimeType>application/wfs-collection-1.1</MimeType>
            </Format>
            <Format>
              <MimeType>application/wfs-collection-1.0</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=gml/3.2.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=gml/3.1.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=gml/2.1.2</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=kml/2.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=kml/2.2</MimeType>
            </Format>
            <Format>
              <MimeType>application/vnd.google-earth.kml+xml</MimeType>
            </Format>
            <Format>
              <MimeType>application/zip</MimeType>
            </Format>
          </Supported>
        </ComplexData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>inputField</ows:Identifier>
        <ows:Title>Z value field</ows:Title>
        <ows:Abstract>The field that holds a height or magnitude value for each point.</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>power</ows:Identifier>
        <ows:Title>The exponent of distance.</ows:Title>
        <ows:Abstract>The exponent of distance: default = 2.0</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>radiusType</ows:Identifier>
        <ows:Title>Search radius type</ows:Title>
        <ows:Abstract>The search radius type: Variable, Fixed</ows:Abstract>
        <LiteralData>
          <ows:AllowedValues>
            <ows:Value>Variable</ows:Value>
            <ows:Value>Fixed</ows:Value>
          </ows:AllowedValues>
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>numberOfPoints</ows:Identifier>
        <ows:Title>Number of points</ows:Title>
        <ows:Abstract>The numberOfPoints is an integer value specifying the number of nearest input sample points to be used to perform the interpolation: default = 12</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:int</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>distance</ows:Identifier>
        <ows:Title>Maximum distance</ows:Title>
        <ows:Abstract>The distance specifies the distance, in map units, by which to limit the search for the nearest input sample points.</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>cellSize</ows:Identifier>
        <ows:Title>Output cell size</ows:Title>
        <ows:Abstract>The cell size for the output gridcoverage</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>extent</ows:Identifier>
        <ows:Title>Extent of the output dataset</ows:Title>
        <ows:Abstract>The extent for the output gridcoverage: WPS BoundingBoxData</ows:Abstract>
        <BoundingBoxData>
          <Default>
            <CRS>EPSG:4326</CRS>
          </Default>
          <Supported>
            <CRS>EPSG:4326</CRS>
          </Supported>
        </BoundingBoxData>
      </Input>
    </DataInputs>
    <ProcessOutputs>
      <Output>
        <ows:Identifier>result</ows:Identifier>
        <ows:Title>Result</ows:Title>
        <ComplexOutput>
          <Default>
            <Format>
              <MimeType>application/arcgrid</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>application/arcgrid</MimeType>
            </Format>
            <Format>
              <MimeType>image/geotiff</MimeType>
            </Format>
            <Format>
              <MimeType>image/tiff</MimeType>
            </Format>
          </Supported>
        </ComplexOutput>
      </Output>
    </ProcessOutputs>
  </ProcessDescription>
</wps:ProcessDescriptions>