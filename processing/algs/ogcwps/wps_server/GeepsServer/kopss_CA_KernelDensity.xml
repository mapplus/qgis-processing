<?xml version="1.0" encoding="utf-8"?>
<wps:ProcessDescriptions xml:lang="en" service="WPS" version="1.0.0" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsAll.xsd" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink">
  <ProcessDescription wps:processVersion="1.0.0" statusSupported="true" storeSupported="true">
    <ows:Identifier>kopss:CA_KernelDensity</ows:Identifier>
    <ows:Title>CA_KernelDensity</ows:Title>
    <ows:Abstract>Calculates a magnitude per unit area from point or polyline features using a kernel function to fit a smoothly tapered surface to each point or polyline.</ows:Abstract>
    <DataInputs>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>inputFeatures</ows:Identifier>
        <ows:Title>Input features</ows:Title>
        <ows:Abstract>The input features (point or line) for which to calculate the density.</ows:Abstract>
        <ComplexData>
          <Default>
            <Format>
              <MimeType>text/xml; subtype=wfs-collection/1.1</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>text/xml; subtype=wfs-collection/1.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=wfs-collection/1.0</MimeType>
            </Format>
            <Format>
              <MimeType>application/json</MimeType>
            </Format>
            <Format>
              <MimeType>application/wfs-collection-1.1</MimeType>
            </Format>
            <Format>
              <MimeType>application/wfs-collection-1.0</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=gml/3.2.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=gml/3.1.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=gml/2.1.2</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=kml/2.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=kml/2.2</MimeType>
            </Format>
            <Format>
              <MimeType>application/vnd.google-earth.kml+xml</MimeType>
            </Format>
            <Format>
              <MimeType>application/zip</MimeType>
            </Format>
          </Supported>
        </ComplexData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>kernelType</ows:Identifier>
        <ows:Title>Kernel function</ows:Title>
        <ows:Abstract>BINARY, COSINE, DISTANCE, EPANECHNIKOV, GAUSSIAN, INVERSE_DISTANCE, QUADRATIC(default), QUARTIC, TRIANGULAR, TRIWEIGHT</ows:Abstract>
        <LiteralData>
          <ows:AllowedValues>
            <ows:Value>BINARY</ows:Value>
            <ows:Value>COSINE</ows:Value>
            <ows:Value>DISTANCE</ows:Value>
            <ows:Value>EPANECHNIKOV</ows:Value>
            <ows:Value>GAUSSIAN</ows:Value>
            <ows:Value>INVERSE_DISTANCE</ows:Value>
            <ows:Value>QUADRATIC</ows:Value>
            <ows:Value>QUARTIC</ows:Value>
            <ows:Value>TRIANGULAR</ows:Value>
            <ows:Value>TRIWEIGHT</ows:Value>
            <ows:Value>TRICUBE</ows:Value>
          </ows:AllowedValues>
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>populationField</ows:Identifier>
        <ows:Title>Field denoting population values for each feature.</ows:Title>
        <ows:Abstract>Values in the population field may be integer or floating point.</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>searchRadius</ows:Identifier>
        <ows:Title>The search radius within which to calculate density.</ows:Title>
        <ows:Abstract>The default is the shortest of the width or height of the output extent in the output spatial reference, divided by 30.</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>cellSize</ows:Identifier>
        <ows:Title>Output cell size</ows:Title>
        <ows:Abstract>The cell size for the output gridcoverage</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>extent</ows:Identifier>
        <ows:Title>Extent of the output dataset</ows:Title>
        <ows:Abstract>The extent for the output gridcoverage</ows:Abstract>
        <BoundingBoxData>
          <Default>
            <CRS>EPSG:4326</CRS>
          </Default>
          <Supported>
            <CRS>EPSG:4326</CRS>
          </Supported>
        </BoundingBoxData>
      </Input>
    </DataInputs>
    <ProcessOutputs>
      <Output>
        <ows:Identifier>result</ows:Identifier>
        <ows:Title>Result</ows:Title>
        <ComplexOutput>
          <Default>
            <Format>
              <MimeType>application/arcgrid</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>application/arcgrid</MimeType>
            </Format>
            <Format>
              <MimeType>image/geotiff</MimeType>
            </Format>
            <Format>
              <MimeType>image/tiff</MimeType>
            </Format>
          </Supported>
        </ComplexOutput>
      </Output>
    </ProcessOutputs>
  </ProcessDescription>
</wps:ProcessDescriptions>