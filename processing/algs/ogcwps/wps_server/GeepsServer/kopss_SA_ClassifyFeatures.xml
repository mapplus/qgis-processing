<?xml version="1.0" encoding="utf-8"?>
<wps:ProcessDescriptions xml:lang="en" service="WPS" version="1.0.0" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsAll.xsd" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink">
  <ProcessDescription wps:processVersion="1.0.0" statusSupported="true" storeSupported="true">
    <ows:Identifier>kopss:SA_ClassifyFeatures</ows:Identifier>
    <ows:Title>SA_ClassifyFeatures</ows:Title>
    <ows:Abstract>Classify objects apply one of several methods to statistically subdivide a set of numeric values into classes.</ows:Abstract>
    <DataInputs>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>inputFeatures</ows:Identifier>
        <ows:Title>1. Input features</ows:Title>
        <ows:Abstract>inputFeatures</ows:Abstract>
        <ComplexData>
          <Default>
            <Format>
              <MimeType>text/xml; subtype=wfs-collection/1.1</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>text/xml; subtype=wfs-collection/1.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=wfs-collection/1.0</MimeType>
            </Format>
            <Format>
              <MimeType>application/json</MimeType>
            </Format>
            <Format>
              <MimeType>application/wfs-collection-1.1</MimeType>
            </Format>
            <Format>
              <MimeType>application/wfs-collection-1.0</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=gml/3.2.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=gml/3.1.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=gml/2.1.2</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=kml/2.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=kml/2.2</MimeType>
            </Format>
            <Format>
              <MimeType>application/vnd.google-earth.kml+xml</MimeType>
            </Format>
            <Format>
              <MimeType>application/zip</MimeType>
            </Format>
          </Supported>
        </ComplexData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>fieldName</ows:Identifier>
        <ows:Title>2. Numeric field</ows:Title>
        <ows:Abstract>The numeric field containing attribute values used to calculate frequency.</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>methodName</ows:Identifier>
        <ows:Title>3. Classification method</ows:Title>
        <ows:Abstract>Classification method: NaturalBreaks(default), Quantile, EqualInterval, StandardDeviation</ows:Abstract>
        <LiteralData>
          <ows:AllowedValues>
            <ows:Value>NaturalBreaks</ows:Value>
            <ows:Value>Quantile</ows:Value>
            <ows:Value>EqualInterval</ows:Value>
            <ows:Value>StandardDeviation</ows:Value>
          </ows:AllowedValues>
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>numClasses</ows:Identifier>
        <ows:Title>4. Number of classes</ows:Title>
        <ows:Abstract>the number of classes</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:int</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>deviationInterval</ows:Identifier>
        <ows:Title>5. Deviation interval(StandardDeviation only)</ows:Title>
        <ows:Abstract>The interval is expressed as a percentage of 1 standard deviation. The value range that may be entered is 0.25 to 1.0(default)</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
    </DataInputs>
    <ProcessOutputs>
      <Output>
        <ows:Identifier>result</ows:Identifier>
        <ows:Title>Result</ows:Title>
        <ComplexOutput>
          <Default>
            <Format>
              <MimeType>text/xml</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>text/xml</MimeType>
            </Format>
          </Supported>
        </ComplexOutput>
      </Output>
    </ProcessOutputs>
  </ProcessDescription>
</wps:ProcessDescriptions>