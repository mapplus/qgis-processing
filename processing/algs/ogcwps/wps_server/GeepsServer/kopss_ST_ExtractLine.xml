  <ProcessDescription wps:processVersion="1.0.0" statusSupported="true" storeSupported="true">
    <ows:Identifier>kopss:ST_ExtractLine</ows:Identifier>
    <ows:Title>Returns the Point on the line at the given distance or ratio.</ows:Title>
    <ows:Abstract>Returns the Point on the line at the given distance or ratio.</ows:Abstract>
    <DataInputs>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>linearGeometry</ows:Identifier>
        <ows:Title>linearGeometry</ows:Title>
        <ows:Abstract>The linear(LineString or Polygon) geometry</ows:Abstract>
        <ComplexData>
          <Default>
            <Format>
              <MimeType>text/xml; subtype=gml/3.1.1</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
