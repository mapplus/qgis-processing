<?xml version="1.0" encoding="utf-8"?>
<wps:ProcessDescriptions xml:lang="en" service="WPS" version="1.0.0" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsAll.xsd" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink">
  <ProcessDescription wps:processVersion="1.0.0" statusSupported="true" storeSupported="true">
    <ows:Identifier>kopss:VA_CalculateFuzzy</ows:Identifier>
    <ows:Title>VA_CalculateFuzzy</ows:Title>
    <ows:Abstract>Transforms the input raster into a 0 to 1 scale, indicating the strength of a membership in a set, based on a specified fuzzification algorithm.</ows:Abstract>
    <DataInputs>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>inputFeatures</ows:Identifier>
        <ows:Title>Input features</ows:Title>
        <ows:Abstract>The input features to be processed.</ows:Abstract>
        <ComplexData>
          <Default>
            <Format>
              <MimeType>text/xml; subtype=wfs-collection/1.1</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>text/xml; subtype=wfs-collection/1.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=wfs-collection/1.0</MimeType>
            </Format>
            <Format>
              <MimeType>application/json</MimeType>
            </Format>
            <Format>
              <MimeType>application/wfs-collection-1.1</MimeType>
            </Format>
            <Format>
              <MimeType>application/wfs-collection-1.0</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=gml/3.2.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=gml/3.1.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=gml/2.1.2</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=kml/2.1</MimeType>
            </Format>
            <Format>
              <MimeType>text/xml; subtype=kml/2.2</MimeType>
            </Format>
            <Format>
              <MimeType>application/vnd.google-earth.kml+xml</MimeType>
            </Format>
            <Format>
              <MimeType>application/zip</MimeType>
            </Format>
          </Supported>
        </ComplexData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>sourceField</ows:Identifier>
        <ows:Title>Source field</ows:Title>
        <ows:Abstract>sourceField</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>targetField</ows:Identifier>
        <ows:Title>Target field that will be updated</ows:Title>
        <ows:Abstract>targetField</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>fuzzyType</ows:Identifier>
        <ows:Title>Fuzzy membership function</ows:Title>
        <ows:Abstract>LinearCutoff(default), LinearOneTrangular, LinearTwoTrangular, CurveSigmoidal, CurveGaussian, CurveBell</ows:Abstract>
        <LiteralData>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>isDecreasing</ows:Identifier>
        <ows:Title>Is Decreasing</ows:Title>
        <ows:Abstract>True or False</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:boolean</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>minValue</ows:Identifier>
        <ows:Title>minValue</ows:Title>
        <ows:Abstract>minValue</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="1">
        <ows:Identifier>maxValue</ows:Identifier>
        <ows:Title>maxValue</ows:Title>
        <ows:Abstract>maxValue</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>midValue1</ows:Identifier>
        <ows:Title>midValue1</ows:Title>
        <ows:Abstract>midValue1</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>midValue2</ows:Identifier>
        <ows:Title>midValue2</ows:Title>
        <ows:Abstract>midValue2</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
      <Input maxOccurs="1" minOccurs="0">
        <ows:Identifier>maxRange</ows:Identifier>
        <ows:Title>maxRange</ows:Title>
        <ows:Abstract>maxRange: 0 ~ maxRange, default = 1</ows:Abstract>
        <LiteralData>
          <ows:DataType>xs:double</ows:DataType>
          <ows:AnyValue />
        </LiteralData>
      </Input>
    </DataInputs>
